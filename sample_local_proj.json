	{
		"model_name": "Human Count Crosslink-NX-VGG-SqDet",
		"git_url": "https://github.com/LatticeSemi/Human-Count.git -b Crosslink-NX-VGG-SqDet-layer-freezing",
		"model_spec": {
			"target_device": "Crosslink-NX",
			"target_application": "Object Detection",
			"target_class": "CNN",
			"input_dim": "224x224",
			"target_network": "SqueezeDet",
			"target_framework": "TensorFlow",
			"target_framework_version": "1.14.0",
			"sensAI_version": "['V2.1.0-Beta', 'V3.0', 'V3.1']",
			"dataset_type": "KITTI",
			"other_param": ""
		},
		"sections": {
			"training": {
				"_comment": "Optional parameter: This determines that whether train section is available or not.",
				"command": {
					"run_type": "python3"
				},
				"execution_script": {
					"name": "Execution Script",
					"default": "src/train.py",
					"type": "file",
					"required": true
				},
				"Model Matrices": {
					"step:loss": "step (\\d+), loss = (\\d+.\\d+)",
					"conf_loss": "conf_loss: (\\d+.\\d+)",
					"bbox_loss": "bbox_loss: (\\d+.\\d+)",
					"class_loss": "class_loss: (\\d+.\\d+)"
				},
				"dataset": {
					"name": "Dataset Type",
					"default": "KITTI",
					"type": "str",
					"required": true
				},
				"net": {
					"name": "Architecture",
					"default": "squeezeDet",
					"type": "str",
					"required": true
				},
				"data_path": {
					"name": "Dataset Path",
					"type": "dir",
					"sub_type": "dataset",
					"required": true
				},
				"train_dir": {
					"name": "Checkpoint Path",
					"type": "dir",
					"sub_type": "checkpoint",
					"required": true
				},
				"image_set": {
					"name": "Image Set",
					"default": "train",
					"type": "str",
					"required": true
				},
				"max_steps": {
					"name": "Max Steps",
					"default": "250000",
					"type": "int",
					"required": true
				},
				"checkpoint_step": {
					"name": "Checkpoint Interval",
					"default": "1000",
					"type": "int",
					"required": true
				},
				"gpu": {
					"name": "Visible GPUs",
					"default": "0",
					"type": "int",
					"required": true
				},
				"transfer_learning": {
					"name": "Transfer Learning",
					"type": "dropdown",
					"required": false,
					"options": {
						"Lattice Github": "git_hub",
						"Local": "local",
						"From other Job": "other_job"
					},
					"url": "https://raw.githubusercontent.com/LatticeSemi/Model_Info/dev/pre_trained_models/cnx_vgg_sqdet_human_count.zip"
				},
				"freeze_layers" : {
					"name": "Freeze Layers",
					"type": "checkboxes",
					"options": {
						"fire1": "fire1",
						"fire2": "fire2",
						"fire3": "fire3",
						"fire4": "fire4",
						"fire5": "fire5",
						"fire6": "fire6",
						"fire7": "fire7",
						"conv12": "conv12"
					},
					"default": "",
					"required": false
				}
			},
			"tensorboard": {
				"_comment": "optional",
				"command": {
					"run_type": "tensorboard"
				},
				"logdir": {
					"name": "Eventfile Path",
					"type": "dir",
					"sub_type": "artefacts",
					"required": true
				},
				"port": {
					"name": "Tensorboard Port",
					"default": "6006",
					"type": "int",
					"required": true
				}
			},
			"freeze": {
				"_comment": "optional",
				"command": {
					"run_type": "python3"
				},
				"execution_script": {
					"name": "Execution Script",
					"default": "src/genpb.py",
					"type": "file",
					"required": true
				},
				"ckpt_dir": {
					"name": "Checkpoint Path",
					"type": "dir",
					"sub_type": "artefacts",
					"required": true
				},
				"freeze": {
					"default": "",
					"type": "flag",
					"required": true
				}
			},
			"map_calculation" : {
				"gpu_option" : false,
                "metrics_option" : true,

				"command": {
					"run_type": "python3"
				},
				"execution_script": {
					"name": "Execution Script",
					"default": "src/map.py",
					"type": "file",
					"required": true
				},
				"Model Matrices": {
					"mAP": "mAP = (\\d+.\\d+)%"

				},
				"pb": {
					"name": "Tensorflow Model File",
					"type": "file",
					"extenstions": "pb",
					"required": true
				},
				"dataset_path": {
					"name": "Dataset Path",
					"type": "dir",
					"sub_type": "dataset",
					"required": true
				},

				"input_node": {
					"name": "Input Node",
					"default": "batch:0",
					"type": "str",
					"required": true
				},
				"output_node": {
					"name": "Output Node",
					"default": "conv12/convolution:0",
					"type": "str",
					"required": true
				},
				"test_set": {
					"name": "Set Type: test/val",
					"default": "val",
					"type": "str",
					"required": true
				},

				"channels":{
					"name":" Number of channels",
					"default": "1",
					"type": "int",
					"required": true
				}
			}
		},
		"SensAI": {
			"device": "CrossLink-NX",
			"scale": 0.0078125,
			"required_output_depth_range": "7-41"
		}
	}